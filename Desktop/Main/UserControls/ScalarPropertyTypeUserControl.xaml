<UserControl x:Class="Main.UserControls.ScalarPropertyTypeUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel>
        <TextBlock>Escalar</TextBlock>
        <Slider Interval="{Binding ScalarPropertyType.TypeOfValue.Step}" Maximum="{Binding ScalarPropertyType.TypeOfValue.MaxValue}" Minimum="{Binding ScalarPropertyType.TypeOfValue.MinValue}" />
    </StackPanel>
</UserControl>
