<Window x:Class="Main.PropertyTypePopup"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="PropertyTypePopup" Height="300" Width="300">
    <DockPanel LastChildFill="True">
        <Grid DockPanel.Dock="Bottom">
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            
            <Button Grid.Column="1" x:Name="okButton" Content="OK" Click="okButton_Click" />
            <Button Grid.Column="0" x:Name="cancelButton" Content="Cancel" Click="cancelButton_Click"/>
        </Grid>
        <StackPanel x:Name="placeholder"></StackPanel>
    </DockPanel>
</Window>
